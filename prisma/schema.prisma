generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name    String?   @db.VarChar(255)
  email   String?   @db.VarChar(255)
  address  String?   @db.VarChar(255)
}

